public boolean checkAdjacent(Board b) throws AdjacentTilesException{
        ArrayList<Tile> chad = new ArrayList<Tile>();
        chad=b.getAdjacent();

        for (Tile k : chad){
                if(chad.get(k).getSymbol != 's'){
                        return true;
                        
                        
                } 
                else{
                return false;
                if(verbose == true){
                        throw new AdjacentTilesException();
                        
                        }
                        }
        }
}
